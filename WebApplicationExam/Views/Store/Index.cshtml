@{
    ViewData["Title"] = "Store";
}

<div class="text-center">
    <h1>Store</h1>
    <table>
        <tr>
            <th>Product Name</th>
            <th>Price</th>
        </tr>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.Cost</td>
            </tr>
        }
    </table>

</div>
