@{
    ViewData["Title"] = "Orders";
}

<div class="text-center">
    <h1>My Orders</h1>
    <table>
        <tr>
            <th>Order ID</th>
            <th>Net Price</th>
        </tr>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.NetPrice</td>
            </tr>
        }
    </table>

</div>
